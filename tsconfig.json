{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es2017",
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": "./src",
        "incremental": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noImplicitReturns": false,
        "strictFunctionTypes": true,
        "forceConsistentCasingInFileNames": true,
        "importHelpers": false
    }
}
